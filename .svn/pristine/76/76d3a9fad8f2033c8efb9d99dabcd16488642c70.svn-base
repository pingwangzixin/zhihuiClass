<!DOCTYPE html>
<html lang="en" class="no-js" ng-app="app">

<head>
    <meta charset="utf-8">
    <title>捷成互动课堂--智慧教学系统</title>
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="full-screen" content="yes">
    <meta name="x5-fullscreen" content="true">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <link rel="stylesheet" href="stylesheets/reset.css">
	<link rel="stylesheet" href="stylesheets/duibi.css" />
    <link rel="stylesheet" href="stylesheets/main.css" />
    <link rel="stylesheet" href="stylesheets/iconfont/iconfont.css" />
    <link rel="stylesheet" href="stylesheets/animate.min.css" />
    <link rel="stylesheet" href="javascripts/mCustomScrollbar/jquery.mCustomScrollbar.css" />
	
	<link rel="stylesheet" href="stylesheets/clock.css">
	<script src="javascripts/rem.js"></script>
    <script src="javascripts/jquery-1.8.2.min.js"></script>
    <script src="javascripts/angular-1.3.2.min.js"></script>
    <script src="javascripts/angular-sanitize.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
	<script type="text/javascript" src="javascripts/rtcClient.js"></script>
    <style type="text/css">

		.caozuo {
		    position: fixed;
		}
		
		.page_btn{
			position: fixed;
			background: #f00;
			width:100px;
			height:100px;
			top: 50%;
			outline: none;
			text-align: center;
			padding: 0;
			border-radius: 50%;
			color: #fff;
			opacity: .3;
			background:#2d2c2c;
		}
		.page_btn>em{
			font-size:60px;
		}
		.lastPage{
			left: 0;
		}
		
		.nextPage{
			right: 0;
		}
		
		#remoteVideosContainer video{
			width:100%;
			height:100%;
		}
    </style>
</head>

<body ng-controller="teachRoom" id="body" ondragstart="window.event.returnValue=false" oncontextmenu="window.event.returnValue=false" onselectstart="event.returnValue=false">

    <!-- 加载进度条 -->
	<div class="tipsNullcontainer" style="display:none">
		<div class="loadEffect">
		    <span></span>
		    <span></span>
		    <span></span>
		    <span></span>
		    <span></span>
		    <span></span>
		    <span></span>
		    <span></span>
		</div>
		<p class="tipData"></p>
	</div>

   <div class="round" id="fd">
   	<div class="toggle" id="toggle" onClick="expand()">
   		<i class="iconfont icon-hao" id="plus"></i>
   	</div>
   	<div class="parent">
   	<div class="menu" id="menu">
   		<div class="circle_bg">
   			<div class="part bg-blue pos01" ng-click="search()">
   				<p class="cont">
   					<i class="iconfont icon-xingtaiduICON_sousuo--"></i>
   					<span>搜索</span>
   				</p>
   			</div>
   			<div class="part bg-red pos02" ng-click="callName()">
   				<p class="cont">
   					<i class="iconfont icon-dianming"></i>
   					<span>点名</span>
   				</p>
   			</div>
   			<div class="part bg-green pos03" ng-click="moretoolsClick()">
   				<p class="cont">
   					<i class="iconfont icon-bangdingyinxingqia"></i>
   					<span>更多</span>
   				</p>
   			</div>
   			<div class="part bg-blue1 pos04">
   				<p class="cont">
   					<i class="iconfont icon-chexiao"></i>
   					<span>撤销</span>
   				</p>
   			</div>
   			<div class="part bg-red2 pos05">
   				<p class="cont">
   					<i class="iconfont icon-pizhu"></i>
   					<span>批注</span>
   				</p>
   			</div>
   			<div class="part bg-green3 pos06" ng-click="countDown()">
   				<p class="cont">
   					<i class="iconfont icon-icon-test"></i>
   					<span>倒计时</span>
   				</p>
   			</div>
   		</div>
   	</div>
   	<!--批注内容 start-->
   	<div id="annotation" style="display: none;">
   		<ul class="color">
   			<li class="actives"><span></span></li>
   			<li><span></span></li>
   			<li><span></span></li>
   			<li><span></span></li>
   			<li><span></span></li>
   			<li><span></span></li>
   		</ul>
   		<ul class="spot">
   			<li  class="actives"><span></span></li>
   			<li><span></span></li>
   			<li><span></span></li>
   		</ul>
   	</div>
   	<!--批注内容 end-->
	<!--批注内容 start-->
	<div id="moreTools" style="display: none;">
		<ul class="moreclass">
			<li ng-click="qiangdaBtn()">
				<img src="images/qiangdabtn.png" alt="">
				<p>抢答</p>
			</li>
		</ul>
	</div>
	
   	</div>
   </div>
   
   <!--批注弹框-->
   <div class="annotationFrame">
   	<canvas id="canvas"></canvas>
   </div>

    <!-- 点名器 -->
    <div class='luck-back' style="display:none">
        <div class="luck-content ce-pack-end">
            <div id="luckuser" class="slotMachine">
                <div class="slot">
                    <span class="name">姓名</span>
                </div>
            </div>
        </div>
    </div>

    <!--登录提示框-->
    <p class="welcome_user" style="display:none;z-index:1200"></p>

    <!--底部固定导航-->
    <div class="dockposition animated bounceInDown">
        <div class="content">
            <div class="back"></div>
            <div class="icons">
                <div class="icon" id="userWindow" ng-click="userWindow()"> 
					<i class="iconfont icon-yonghu"></i>
                    <div class="title">用户</div>
                </div>
                <div class="icon" id="memberAdmin" ng-click="memberAdmin()"> <i class="iconfont icon-yonghu1"></i>
                    <div class="title">成员</div>
                </div>
                <div class="icon" id="classRecord" ng-click="classRecorda()"> <i class="iconfont icon-jilu1"></i>
                    <div class="title">教学记录</div>
                </div>
                <div class="icon" id="classBegins" ng-click="classBegins()"> <i class="iconfont icon-lingdang1"></i>
                    <div class="title">上课</div>
                </div>
                <div class="icon" id="resouceAdmin" ng-click="resouceAdmin()"> <i class="iconfont icon-yun1"></i>
                    <div class="title">资源</div>
                </div>
                <div class="icon" id="drawclick" ng-click="drawclick()"> <i class="iconfont icon-heibanblackboard4"></i>
                    <div class="title">白板</div>
                </div>
                <div class="icon" id="yunpingtai" ng-click="yunpingtai()"> <i class="iconfont icon-xuanti-pingtai"></i>
                    <div class="title">平台</div>
                </div>
                <div class="icon" id="exitid" ng-click="exitid()"> <i class="iconfont icon-qunfenganquantuichu"></i>
                    <div class="title">退出</div>
                </div>
            </div>
        </div>
    </div>
    
    <!--弹窗-->
    <div class="content appcontwindow">
        <!--上课弹窗-->
        <div class="showTc classTc" style="display: none;">
            <div class="tchide"></div>
            <div class="tcContent">
                <div class="box_title"> <span class="close_box fr" ng-click="closeWindow($event)"><img src="images/close.svg" alt="" /></span> <span class="title_box_tc">教学</span> </div>
                <div class="banjinianji clearfix">
                    <div> 班级:
                        <select id="banjiid" ng-model="grade.classId" ng-change="selectClass(grade.classId)" ng-options="grade.classId as grade.gradeName+grade.className+'班' for grade in gradeClass">
                            <option value="">请选择班级</option>
                        </select>
                    </div>
                    <div> 科目:
                        <select id="xuekeid" ng-model="sbjid">
                            <option value="">请选择科目</option>
                        </select>
                    </div>
                    <div style="margin-top: 20px;"> 录屏:
						<div style="width:27rem;display:inline-block;text-align: justify;">
							<input name="luping" id="screenyes" type="radio"><label for="screenyes">是</label>
							<input name="luping" id="screenno" type="radio" checked><label for="screenno">否</label>
						</div>
                        
                    </div>
                </div>
                <button class="sure_btn" ng-click="sure_beginClass(grade.classId,sbjid)">确认</button>
            </div>
        </div>
        <!--下课确认框-->
        <div class="showTc xiakeTc" style="display: none;">
            <div class="tchide"></div>
            <div class="tcContent">
                <div class="box_title"> <span class="close_box fr" ng-click="closeWindow($event)"><img src="images/close.svg" alt="" /></span> <span class="title_box_tc">提示</span> </div>
                <div class="text_shadow tips_div">
                    <p class="xiaketips" style="display: none;">确认下课？</p>
                </div> <span class="btn surecolor xiake_sure" ng-click="xiake_sure()">确认</span> <span class="btn cancelcolor color_hui logoutcancel" ng-click="closeWindow($event)">取消</span> </div>
        </div>
        <!--退出确认框-->
        <div class="showTc signOutTc userLogOut" style="display: none;">
            <div class="tchide"></div>
            <div class="tcContent">
                <div class="box_title"> <span class="close_box fr" ng-click="closeWindow($event)"><img src="images/close.svg" alt="" /></span> <span class="title_box_tc">提示</span> </div>
                <div class="text_shadow tips_div">
                    <p>确认退出？</p>
                </div> 
				<span class="btn surecolor logout_sure" ng-click="logout_sure()">确认</span> 
				<span class="btn cancelcolor color_hui logoutcancel" ng-click="closeWindow($event)">取消</span> 
			</div>
        </div>
        <!--用户信息弹窗-->
        <div class="showTc ctrlTc userInfoTc" style="display: none;">
            <div class="tchide"></div>
            <div class="tcContent animated  fadeInLeft">
                <div class="box_title"> <span class="close_box fr" ng-click="closeWindow($event)"><img src="images/close.svg" alt="" /></span> <span class="title_box_tc">个人信息</span> </div>
                <div class="wx_contentBox userinfoCont">
                    <!--用户信息-->
                    <ul class="fl userinfoul">
                        <li>用户名:<span ng-bind="userName"></span></li>
                        <li>手机号:<span ng-bind="userMobile"></span></li>
                        <li>邮箱号:<span ng-bind="userEmail"></span></li>
                    </ul> <span class="fl teachFace"><img ng-src="{{teachFace}}" style="width:100%;" /></span> </div>
            </div>
        </div>
        <!--成员管理弹窗-->
        <div class="showTc ctrlTc menberAdminTc" style="display: none;">
            <div class="tchide"></div>
            <div class="tcContent  animated jackInTheBox">
                <div class="box_title"> <span class="close_box fr" ng-click="closeCyglWindow($event)"><img src="images/close.svg" alt="" /></span> <span class="title_box_tc">成员管理</span> </div>
                <div class="wx_contentBox clearfix">
                    <div class="menberight fl">
                        <!---成员列表-->
                        <div class="menber_list_show">
                            <div class="menber_tips"> <span>全部:<i ng-bind="allcount"></i>人</span> <span>已签到:<i ng-bind="comecount"></i>人</span> <span>未签到:<i ng-bind="allcount-comecount"></i>人</span>
                            </div>
                            <div class="memberAdminCon" style="display: block;">
                                <div ng-if="remember.length==0" class="tipsall"> <img style="width:330px" src="images/404.png" />
                                    <p class="tipscolor">没有查询到学生</p>
                                </div>

                                <div ng-repeat="i in remember track by i.stuNo" ng-click="studentFile($event,i.id)" class="chengyuanlist icon opacity" >
                                    <em ng-show="devce.indexOf(i.deviceName)!=-1 || devce.indexOf(i.id)!=-1 " ng-class="{'removeclas':devce.indexOf(i.deviceName)!=-1 || devce.indexOf(i.id)!=-1 }"></em>
                                    <i>
										<img ng-if="i.sex=='男'" src="images/boy.png"/>
										<img ng-if="i.sex=='女'" src="images/girl.png" />
									</i>
                                    <span ng-if="stuUpLoadFileId.indexOf(i.id)!=-1" class="num" ng-bind="stuUpLoadFile[stuUpLoadFileId.indexOf(i.id)].stuCount"></span>
									
                                    <div class="title" ng-bind="i.realname"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        	<div class="tc imgListtc" style="display: none;">
				<span class="closefullscreen fr" ng-click="closestuimguploadtc($event)">
					<em class="iconfont icon-guanbi1 exitResTc"></em>
				</span>
        		<div class="content">
	        		<div class="wx_contentBox  clearfix">
	                    <div class="resouceright resoucerights">
	                        <div>
	                            <div class="ziyuandiv">
	                                <ul class="ziyuanul">
	                                    <li style="width:20rem;display: inline-block;margin: 2rem;" ng-repeat="i in studentFileList track by $index" ng-click="showstuimgResoce(i.resourceUrl,'0')"> 
	                                        <img ng-src="{{i.resourceUrl}}" style="width:20rem">
											<p class="filetext_title" ng-bind="i.resourceName"></p>
	                                    </li>
	                                </ul>
	                            </div>
	                        </div>
	                    </div>
	                </div>
        		</div>
        	</div>
        </div>
        <!--资源弹窗-->
        <div class="showTc ctrlTc resouceInfoTc" style="display: none;">
            <div class="tcContent  animated rollIn">
                <div class="box_title"> <span class="close_box fr" ng-click="closeWindow($event)"><img src="images/close.svg"/></span> <span class="title_box_tc">资源</span> </div>
                <div class="wx_contentBox  clearfix">
                    <div class="resouceleft fl">
                        <ul>
                            <li ng-class="{'wx_active':rs==0}" ng-click="restab(0)"><i class="iconfont icon-yun1"></i>云资源</li>
                            <li ng-class="{'wx_active':rs==1}" ng-click="restab(1)"><i class="iconfont icon-yun1"></i>云试题</li>
                        </ul>
                    </div>
                    <div class="resouceright resoucerights fl">
                        <div ng-show="rs==0">
                            <div class="alldom ziyuandiv" id="all_folder">
                                <div ng-if="resourceList.length==0" class="tipsall"> <img style="width:330px" src="images/404.png"/>
                                    <p class="tipscolor">没有查询到资源</p>
                                </div>
                                <ul class="ziyuanul">
                                    <li ng-repeat="i in resourceList track by $index" ng-click="showResoce(i.resourceId,i.resType)"> <span ng-class="{'wx_video':i.resType==1 || i.resType==8 || i.resType==9,'wx_aduo':i.resType==2,'wx_img':i.resType==3,'wx_pdf':i.resType==4,'wx_ppt':i.resType==5,'wx_doc':i.resType==6,'wx_excel':i.resType==7,'wx_qita':i.resType==null}"></span>
                                        <p class="filetext_title" ng-bind="i.resourceName"></p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div ng-show="rs==1">
                            <div class="alldom shitidiv" id="all_folder">
                                <div ng-if="resourceLists.length==0" class="tipsall"> <img style="width:330px" src="images/404.png"/>
                                    <p class="tipscolor">没有查询到试题</p>
                                </div>
                                <ul class="ziyuanul">
                                    <li ng-repeat="i in resourceLists track by $index" ng-click="showResoceshiti(i.resourceId,i.downloadUrl,i.resType,i.resourceName)"> <span ng-class="{'wx_shiti':true}"></span>
                                        <p class="filetext_title" ng-bind="i.resourceName"></p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--课堂记录弹窗-->
        <div class="showTc ctrlTc recordInfoTc" style="display: none;">
            <div class="tcContent  animated rollIn">
                <div class="box_title"> 
                	<span class="close_box fr" ng-click="closeWindow($event)">
                		<img src="images/close.svg" alt="" />
                	</span> 
                	<span class="title_box_tc">教学记录</span> 
                </div>
                <div class="wx_contentBox  clearfix">
                    <div class="resouceright ">
                        <div class="alldom ziyuandiv" id="all_folder">
                            <div ng-if="recordList.length==0" class="tipsall"> 
                            	<img style="width:330px" src="images/404.png" alt="" />
                                <p class="tipscolor">没有查询到记录</p>
                            </div>
                            <ul class="ziyuanul">
                            	<li ng-repeat="i in recordList track by $index" ng-click="showclassRecord(i.id,i.resourceId,i.resourceUrl,i.type,i.name)"> 
								    <span ng-class="{'wx_img':i.type==2,'wx_img':i.type==3,'wx_doc':i.type==1,'wx_qita':i.type==null}"></span>
                                    <p class="filetext_title" ng-bind="i.name"></p>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--资源展示方式弹窗-->
        <div class="showTc ctrlTc resouceTctips" style="display: none;">
            <div class="resoucetcContent">
                <div class="box_title">
                    <span class="closetest_box fr" ng-click="closeshowType()"><img src="images/close.svg"/></span>
                    <span class="title_box_tc">查看资源</span>
                </div>
                <div class="wx_contentBox  clearfix">
                    <span class="wx_pdbgff"><a target="_blank" href="{{downloadUrl}}">
                <img src="images/doc.png" alt="" /><br />本地查看</a></span>
                    <span ng-if="restypehide" class="wx_pdbgff">
                    <a target="_blank" ng-click="drawclick()"><img src="images/doc.png"/><br />在线查看</a>
                </span>
                </div>
            </div>
        </div>
        <!--视频资源播放器弹窗-->
        <div class="showTc ctrlTc resoucePlayer" style="display: none;">
            <div class="resoucetcContent videoPlayTc">
                <span class="closefullscreen fr" ng-click="closeresdetail()">
					<em class="iconfont icon-guanbi1 exitResTc"></em>
				</span>
                <div class="wx_contentBox video_contentBox clearfix">
					<video src="" controls id="videoplay" style="width: 100%; height: 100%;"></video>
                </div>
            </div>
        </div>
        <!--图片资源展示弹窗-->
        <div class="showTc ctrlTc imgPlayer player" style="display: none;">
            <div class="resoucetcContent">
                <span class="closefullscreen fr" ng-click="closeimgtc()">
					<em class="iconfont icon-guanbi1 exitResTc"></em>
                </span>
				
				<span class="minimizeResources" id="imgMinimize"  ng-show="onePic==true">
					<em class="iconfont icon-zuixiaohua"></em>
				</span>
				<div class="imgZoom">
					<button type="button" class="iconfont icon-fangda2" id="big"></button>
					<button type="button" class="iconfont icon-suoxiao" id="small"></button>
					<button type="button" class="iconfont icon-zuojiantou" id="left"></button>
					<button type="button" class="iconfont icon-youjiantou" id="right"></button>
					<button type="button" class="iconfont icon-shangjiantou" id="top"></button>
					<button type="button" class="iconfont icon-xiajiantou" id="bottom"></button>
				    <button type="button" class="iconfont icon-youxuanzhuan" id="zuoxuanzhuan"></button>
				</div>
                <div class="wx_contentBox  clearfix wx_transform" style="text-align:center;"> 
					<img class="wx_imgshowtc" ng-src="{{imgresShowtc}}" alt="" id="wx_imgshowtc"/> 
				</div>
				<div class="caozuo" ng-if="onePic==false&&noduibi==false">
				    <button ng-click="leftChangeImg()" class="page_btn lastPage"><em class="iconfont icon-zuojiantou"></em></button>
				    <button ng-click="rightChangeImg()" class="page_btn nextPage"><em class="iconfont icon-youjiantou"></em></button>
				</div>
            </div>
        </div>
		<!--图片对比投屏-->
		<div class="showTc ctrlTc imgduibiPlayer" style="display:none;">
		    <div class="resoucetcContent">
		        <span class="closefullscreen fr" ng-click="closeimgduibiPlayer()">
					<em class="iconfont icon-guanbi1 exitResTc"></em>
		        </span>
				<!-- 新加的对比 -->
		        <div class="blocks">
				  <div class="block" ng-repeat="i in imgscontrasts track by i.name"  ng-class="{'left':i.name==2 || i.name==0,'right':i.name==1 || i.name==3,'bot':i.name==2||i.name==3,'top':i.name==0||i.name==1}"  ng-click="contrastsImg(i.name)">
					<h1><img ng-src="{{i.imgUrl}}"></h1>
				  </div>
				</div>
				<!-- 新加的对比 -->
		    </div>
		</div>
        <!--ppt资源展示弹窗-->
        <div class="showTc ctrlTc pptPlayer  player" style="display: none;">
            <div class="resoucetcContent">
                <span class="closefullscreen fr" ng-click="closePpttc('.pptPlayer')">
					<em class="iconfont icon-guanbi1 exitResTc"></em>
		        </span>
				<span class="minimizeResources" id="pptMinimize">
					<em class="iconfont icon-zuixiaohua"></em>
				</span>
                <iframe src="" id="showPPTsrc" frameborder="0"></iframe>
            </div>
        </div>
		<!--word资源展示弹窗-->
		<div class="showTc ctrlTc wordPlayer  player" style="display: none;">
		    <div class="resoucetcContent">
		        <span class="closefullscreen fr" ng-click="closePpttc('.wordPlayer')">
					<em class="iconfont icon-guanbi1 exitResTc"></em>
		        </span>
				<span class="minimizeResources" id="wordMinimize">
					<em class="iconfont icon-zuixiaohua"></em>
				</span>
		        <iframe src="" id="showWordsrc" frameborder="0"></iframe>
		    </div>
		</div>
        <!--excel资源展示弹窗-->
        <div class="showTc ctrlTc excelPlayer  player" style="display: none;">
            <div class="resoucetcContent">
                <span class="closefullscreen fr" ng-click="closePpttc('.excelPlayer')">
        			<em class="iconfont icon-guanbi1 exitResTc"></em>
                </span>
        		<span class="minimizeResources" id="excelMinimize">
        			<em class="iconfont icon-zuixiaohua"></em>
        		</span>
                <iframe src="" id="showExcelsrc" frameborder="0"></iframe>
            </div>
        </div>
		<!--pdf资源展示弹窗-->
		<div class="showTc ctrlTc pdfPlayer  player" style="display: none;">
		    <div class="resoucetcContent">
