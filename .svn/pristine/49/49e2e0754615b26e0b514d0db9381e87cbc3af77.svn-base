html,
body {
	background: url('../images/backgrounds/4.jpg') no-repeat;
	background-size: cover;
	width: 100%;
	height: 100%;
	overflow: hidden;
	-moz-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	-khtml-user-select: none;
	user-select: none;
	position: relative;
	color: #fff;
	font-family: 'PT Sans', Helvetica, Arial, sans-serif;
	text-align: center;
}

::-webkit-scrollbar {
width: 5px;
}
/* 滚动槽 */
::-webkit-scrollbar-track {
-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
border-radius: 5px;
}
/* 滚动条滑块 */
::-webkit-scrollbar-thumb {
border-radius: 5px;
background: #000;
-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5);
}
::-webkit-scrollbar-thumb:window-inactive {
background:#000;
}

strong {
	font-weight: bold;
	font-size: 3.4rem;
}

.colorfff {
	color: #fff;
}

.alldom {
	height: 33rem;
	margin-top: 1rem;
	overflow-y: auto;
}

.alldom ul {
	padding: 0;
	list-style: none;
	padding-top: .1rem;
}

.alldom ul li {
	overflow: hidden;
	width: 9rem;
	height: 12rem;
	float: left;
	margin: .4rem .3rem;
	border: 0;
	position: relative;
	transition: all 0.2s ease-in 0.1s;
	text-align: center;
}

.alldom ul li.focus {
	background-color: #69a9da;
	border: 0;
}

.alldom ul li:hover {
	background-color: #69a9ba;
	border: 0;
}

.clearfix:before,
.clearfix:after {
	content: "";
	display: table;
	clear: both;
}

.teachFace {
	display: inline-block;
	width: 8rem;
	height: 8rem;
	margin-top: 2.6rem;
	border-radius: 50%;
	overflow: hidden;
}

.userinfoul {
	width: 30rem;
}

.clearfix {
	*zoom: 1;
}

.fl {
	float: left;
}

.fr {
	float: right;
}

#supersized-loader {
	position: absolute;
	top: 50%;
	left: 50%;
	z-index: 0;
	width: 6rem;
	height: 6rem;
	margin: -30px 0 0 -30px;
	text-indent: -999em;
}

#supersized {
	display: block;
	position: fixed;
	left: 0;
	top: 0;
	overflow: hidden;
	z-index: -999;
	height: 100%;
	width: 100%;
}

#supersized img {
	width: auto;
	height: auto;
	position: relative;
	display: none;
	outline: none;
	border: none;
}

#supersized.speed img {
	-ms-interpolation-mode: nearest-neighbor;
	image-rendering: -moz-crisp-edges;
}


/*Speed*/

#supersized.quality img {
	-ms-interpolation-mode: bicubic;
	image-rendering: optimizeQuality;
}


/*Quality*/

#supersized li {
	display: block;
	list-style: none;
	z-index: -30;
	position: fixed;
	overflow: hidden;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #111;
}

#supersized a {
	width: 100%;
	height: 100%;
	display: block;
}

#supersized li.prevslide {
	z-index: -20;
}

#supersized li.activeslide {
	z-index: -10;
}

#supersized li.image-loading {
	width: 100%;
	height: 100%;
}

#supersized li.image-loading img {
	visibility: hidden;
}

#supersized li.prevslide img,
#supersized li.activeslide img {
	display: inline;
}

#supersized img {
	max-width: none !important
}

