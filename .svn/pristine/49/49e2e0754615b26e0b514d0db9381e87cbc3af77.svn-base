	-o-transition: all .2s;
	-moz-transition: all .2s;
	-webkit-transition: all .2s;
	-ms-transition: all .2s;
}

#body input:-moz-placeholder {
	color: #fff;
}

#body input:-ms-input-placeholder {
	color: #fff;
}

#body input::-webkit-input-placeholder {
	color: #fff;
}

#body input:focus {
	outline: none;
	-moz-box-shadow: 0 .2rem .3rem 0 rgba(0, 0, 0, .1) inset, 0 2px 7px 0 rgba(0, 0, 0, .2);
	-webkit-box-shadow: 0 .2rem .3rem 0 rgba(0, 0, 0, .1) inset, 0 2px 7px 0 rgba(0, 0, 0, .2);
	box-shadow: 0 .2rem .3rem 0 rgba(0, 0, 0, .1) inset, 0 2px 7px 0 rgba(0, 0, 0, .2);
}

button.sure_btn,
button.login_btn {
